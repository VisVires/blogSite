<div id="photography">
	<div class="col-sm-10">
		<div class="blog-header">
	      	<h1 style="text-align: left" class="blog-title"><strong>MY PHOTOGRAPHY</strong></h1>
	     	<p class="lead blog-description">A collection of my photography from the PNW and my travels around the world</p>
	     	<hr class="style3">
   	 	</div>
		{{#each photo}}
		<img class="img-fluid" src="{{this.photo}}" alt="{{this.photo}}" style="width: 1223px; padding-left: 8px; padding-right: 8px;">
		<div class="caption">
			<p><strong>{{this.location}}</strong>: {{this.caption}}</p>
		</div>
		{{/each}}
		 <div class="fb-comments" data-href="https://www.williamgeorge.me/photography" data-numposts="5"></div>
	</div>
	<div class="col-sm-2">
	</div>
</div>